@CONFERENCE{Weibull_physical,
  author = {Geusebroek, J.M. and Smeulders, A.W.M.},
  title = {A physical explanation for natural image statistics},
  booktitle = {Proceedings of the 2nd International Workshop on Texture Analysis
	and Synthesis (Texture 2002)},
  year = {2002},
  pages = {47--52},
  note = {Observations on 45,000 stock photos of natural images, covering a
	wide variety of topics, revealed that 60% of all images have strict
	Weibull-shaped contrast distributions. The remaining percentage of
	images has a distribution close to Weibull or is highly regular.	These
	images are typically composed of two (or more) parts in the image,
	each having a distinct Weibull contrast distribution.}
}

