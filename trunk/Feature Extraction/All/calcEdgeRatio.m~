function edgeRatio = calcEdgeRatio(image)        
        % Parameters????????
        if size(image,3) == 3
            edgeImage = edge(rgb2gray(image),'canny');
        else 
            edgeImage = edge(image(:,:,1),'canny');
        end
        
        edgeRatio = sum(sum(edgeImage))/(size(edgeImage,1)*size(edgeImage,2));
        
         
end

